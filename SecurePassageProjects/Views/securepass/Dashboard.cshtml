@model CompositeModel
<!DOCTYPE html>
<html lang="en">
<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>

<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Premium Table Design</title>
<style>
    /* Basic styling */
    body {
        font-family: Arial, sans-serif;
        margin: 0;
        padding: 0;
        background-color: #f3f5f8;
    }

        .container {
/*             display: flex;
 */            flex-wrap: wrap;
            justify-content: space-around;
        }

    table {
            width: 43%;
        border-collapse: separate;
        border-spacing: 0;
        border-radius: 10px;
        overflow: hidden;
        box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
        background-color: #fff;
        margin: 50px auto;
    }

    th, td {
        border: none;
        padding: 12px 20px;
        text-align: left;
    }

    th {
        background-color: #242b3b;
        color: #fff;
        font-weight: 600;
        text-transform: uppercase;
    }

    td {
        background-color: #f8f9fa;
        color: #333;
    }

    /* Styling for table header corners */
    th:first-child {
        border-top-left-radius: 10px;
    }

    th:last-child {
        border-top-right-radius: 10px;
    }

    /* Styling for table data corners */
    td:first-child {
        border-bottom-left-radius: 10px;
    }

    td:last-child {
        border-bottom-right-radius: 10px;
    }


 


        @@media(max-width: 500px) {

            .table thead{
                display:none;
            }

            .table, .table tbody, .table tr,.table td{
                display:none;
                width:100%;

            }


        }
    
</style>
</head>
<body>
    <header>


        <nav id="navbar" class="">
            <div class="nav-wrapper">
                <!-- Navbar Logo -->
                <div class="logo">
                    <!-- Logo Placeholder for Illustration -->
                    @*             <a href="#home"><img width="100px" height="auto" src="https://www.codepel.com/wp-content/uploads/2023/01/CodePel-Logo-2.1.png"></a>
                    *@
                    <a href="#home">
                        <img width="60px" height="auto" alt="svgImg" src="data:image/svg+xml;base64," />
                    </a>
                    <a style="font-family:'Courier New'">PassWordManager</a>
                </div>

                <!-- Navbar Links -->
                <ul id="menu">
                    <li><a href="@Url.Action("createaccount","securepass")">Add account</a></li>
                    <!--
                    -->
                    <!--
                    -->
                    <li><a href="@Url.Action("Logiin","securepass")">Logout</a></li>
                </ul>
            </div>
        </nav>

    </header>

    <div class="container" id="container">
        <div>

            @if (Model.GoogleModelss != null && Model.GoogleModelss.Any())
            {
                
<table>
    <thead>
        <tr>
            <th scope="col">AccountType</th>
            <th scope="col">GooglePassword</th>
            <th scope="col">GooglePassword</th>
                        <th scope="col">Edit</th>
                        <th scope="col">Delete</th>

           
        </tr>
    </thead>
    <tbody>
                    @foreach (var googleView in Model.GoogleModelss)
                    {
                        
                    
           <tr>
            <td>@googleView.Accounttypego</td>

            <td >@googleView.GoogleuserName</td>
            <td>@googleView.GooglePassword</td>
            <td>
                                <a asp-action="googleedit" asp-route-Accounttypego="@googleView.Accounttypego" asp-route-Id="@googleView.GoogleId">Edit</a> |
            </td>
                            <td>
                            
                                <form method="post" action="@Url.Action("delete", "securepass")" class="delete-form">
                                    <input type="hidden" name="Accounttypego" value="@googleView.Accounttypego" />
                                    <input type="hidden" name="GoogleuserName" value="@googleView.GoogleuserName" />
                                    <input type="hidden" name="GooglePassword" value="@googleView.GooglePassword" />

                                    <input type="hidden" name="GoogleId" value="@googleView.GoogleId" />
                                    <button id="GoogleButtonClick" class="btn btn-danger">Delete</button>
                                    @Html.AntiForgeryToken()
                                </form>
                            </td>
                 
        </tr>
                    } 
    </tbody>
                </table>
            }

           
        </div>


<div>
      @if (Model.FacebookModelss != null && Model.FacebookModelss.Any())
            {
                <table>
                    <thead>
                        <tr>
                            <th scope="col">AccountType</th>
                            <th scope="col">FBUsername</th>
                            <th scope="col">FBPassword</th>
                            <th scope="col">Edit</th>
                            <th scope="col">Delete</th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var fbView in Model.FacebookModelss)
                        {


                            <tr>
                                <td>@fbView.AccounttypeFb</td>

                                <td>@fbView.FBuserName</td>
                                <td>@fbView.FBPassword</td>
                                <td>
                                    <a asp-action="facebookEdit" asp-route-Accounttypego="@fbView.AccounttypeFb" asp-route-Id="@fbView.FacebookId">Edit</a> |
                                </td>
                                <td>

                                    <form method="post" action="@Url.Action("delete", "securepass")" class="delete-form">
                                        <input type="hidden" name="AccounttypeFb" value="@fbView.AccounttypeFb" />
                                        <input type="hidden" name="FBuserName" value="@fbView.FBuserName" />
                                        <input type="hidden" name="FacebookId" value="@fbView.FacebookId" />
                                        <input type="hidden" name="FBPassword" value="@fbView.FBPassword" />

                                        <input type="hidden" name="FacebookId" value="@fbView.FacebookId" />
                                        <button id="GoogleButtonClick" class="btn btn-danger">Delete</button>
                                        @Html.AntiForgeryToken()
                                    </form>
                                </td>
                            </tr>
                        }

        </tbody>
    </table>
    }
        </div>


   <div>
            @if (Model.InstagramModelss != null && Model.InstagramModelss.Any())
            {
                <table>
                    <thead>
                        <tr>
                            <th scope="col">AccountType</th>
                            <th scope="col">InstaUsername</th>
                            <th scope="col">InstaPassword</th>
                            <th scope="col">Edit</th>
                            <th scope="col">Delete</th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var instaView in Model.InstagramModelss)
                        {


                            <tr>
                                <td>@instaView.AccounttypeIN</td>

                                <td>@instaView.INuserName</td>
                                <td>@instaView.INPassword</td>
                                <td>
                                    <a asp-action="instaedit" asp-route-Accounttypego="@instaView.AccounttypeIN" asp-route-Id="@instaView.InstaId">Edit</a> |
                                </td>
                                <td>

                                    <form method="post" action="@Url.Action("delete", "securepass")" class="delete-form">
                                        <input type="hidden" name="AccounttypeIN" value="@instaView.AccounttypeIN" />
