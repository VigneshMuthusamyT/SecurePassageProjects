                                        <input type="hidden" name="INPassword" value="@instaView.INPassword" />

                                        <input type="hidden" name="InstaId" value="@instaView.InstaId" />
                                        <button id="GoogleButtonClick" class="btn btn-danger">Delete</button>
                                        @Html.AntiForgeryToken()
                                    </form>
                                </td>
                            </tr>
                        }

        </tbody>
    </table>
    }
        </div>

    <div>
            @if (Model.BankingModelss != null && Model.BankingModelss.Any())
            {
                <table>
                    <thead>
                        <tr>
                            <th scope="col">AccountType</th>
                            <th scope="col">Username</th>
                            <th scope="col">Phonenumber</th>
                            <th scope="col">Upin</th>
                            <th scope="col">Edit</th>
                            <th scope="col">Delete</th>




                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var bankingView in Model.BankingModelss)
                        {


                            <tr>
                                <td>@bankingView.accounttype</td>

                                <td>@bankingView.Bankname</td>
                                <td>@bankingView.BankPhonenumber</td>
                                <td>@bankingView.UPIPin</td>

                                <td>
                                    <a asp-action="bankingedit" asp-route-Accounttypego="@bankingView.accounttype" asp-route-Id="@bankingView.bankId">Edit</a> |
                                </td>
                                <td>

                                    <form method="post" action="@Url.Action("delete", "securepass")" class="delete-form">
                                        <input type="hidden" name="accounttype" value="@bankingView.accounttype" />
                                        <input type="hidden" name="bankId" value="@bankingView.bankId" />
                                        <input type="hidden" name="BankPhonenumber" value="@bankingView.BankPhonenumber" />
                                        <input type="hidden" name="Bankname" value="@bankingView.Bankname" />
                                        <input type="hidden" name="UPIPin" value="@bankingView.UPIPin" />


                                        <input type="hidden" name="bankId" value="@bankingView.bankId" />
                                        <button id="GoogleButtonClick" class="btn btn-danger">Delete</button>
                                        @Html.AntiForgeryToken()
                                    </form>
                                </td>
                            </tr>
                        } 

        </tbody>
    </table>
    }
        </div>

    </div>

    <script type="text/javascript">
        $(document).ready(function () {
            $("#GoogleButtonClicks").click(function (e) {
                // var accountType = $this.data('Accounttypego');
                // var googleId = $this.data('GoogleId');
                $.ajax({
                    url: "/securepass/delete",
                    type: "POST",
                    dataType: 'json',
                    //  contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                    // data: {
                    //     Accounttypego: accountType,
                    //     GoogleId: googleId
                    // },
                    success: function (response) {
                        if (response.success) {
                            // Reset form after successful submission
                            $("#friendforminsta")[0].reset();
                        }

                        // Reset form after successful submission
                    }



                });
            });
        });
    </script>
</body>
</html>
